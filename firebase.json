{
  "hosting": {
    "public": "public",
    "headers": [ {
      "source" : "**/*.@(jpg|jpeg|gif|png|eot|otf|ttf|ttc|woff|css|js)",
      "headers" : [ {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ],
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=86400"
      } ]
      }, {
      "source" : "404.html",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=86400"
      } ]
    } ],
    "cleanUrls": true
  }
}
